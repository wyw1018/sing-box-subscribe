{
  "dns": {
    "rules": [
      {  
         "outbound": "any",
         "server": "ALIDNS_DOH",
         "disable_cache": true
      },
      {
         "clash_mode": "Global",
         "server": "GOOGLEDNS_DOH"
      },      
      {
        "query_type": [
          "A",
          "AAAA"
        ],
        "server": "fakedns"
      }
    ],
    "servers": [
      {
        "address": "https://8.8.8.8/dns-query",
        "strategy": "ipv4_only",
        "detour": "select",
        "tag": "GOOGLEDNS_DOH"
      },
      {
        "address": "https://223.5.5.5/dns-query",
        "strategy": "ipv4_only",
        "detour": "direct",
        "tag": "ALIDNS_DOH"
      },
      {
        "tag": "fakedns",
        "address": "fakeip"
      }
    ],
    "fakeip": {
      "enabled": true,
      "inet4_range": "28.0.0.1/8"
    },
    "independent_cache": true,
    "strategy": "ipv4_only"
  },
  "experimental": {
    "cache_file": {
      "enabled": true,
      "store_fakeip": false
    },
    "clash_api": {
      "external_controller": "127.0.0.1:9090",
      "external_ui": "xd",
      "external_ui_download_detour": "select",
      "external_ui_download_url": "https://mirror.ghproxy.com/https://github.com/MetaCubeX/metacubexd/archive/refs/heads/gh-pages.zip",
      "default_mode": "Rule"
    }
  },
  "inbounds": [
    {
      "auto_route": true,
      "inet4_address": "172.19.0.1/30",
      "mtu": 9000,
      "sniff": true,
      "stack": "gvisor",
      "strict_route": true,
      "type": "tun"
    }
  ],
  "log": {
    "level": "debug",
    "timestamp": true
  },
  "outbounds": [
    {
      "tag": "select",
      "type": "selector",
      "outbounds": [
        "direct",
        "{all}"
      ]
    },
    {
      "tag": "direct",
      "type": "direct"
    },
    {
      "tag": "block",
      "type": "block"
    },
    {
      "tag": "dns",
      "type": "dns"
    }
  ],
  "route": {
    "auto_detect_interface": true,
    "override_android_vpn": true,
    "rule_set": [
            {
                "type": "remote",
                "tag": "geosite-geolocation-cn",
                "format": "binary",
                "url": "https://raw.githubusercontent.com/SagerNet/sing-geosite/rule-set/geosite-geolocation-cn.srs"
            },
            {
                "type": "remote",
                "tag": "geoip-cn",
                "format": "binary",
                "url": "https://raw.githubusercontent.com/SagerNet/sing-geoip/rule-set/geoip-cn.srs"
            },
            {
                "tag": "GeoSite-Google-CN",
                "type": "remote",
                "format": "binary",
                "url": "https://raw.githubusercontent.com/Dreista/sing-box-rule-set-cn/rule-set/google.china.conf.srs"
            },        
            {
               "tag": "geosite-private",
               "type": "remote",
               "format": "binary",
               "url": "https://testingcf.jsdelivr.net/gh/MetaCubeX/meta-rules-dat@sing/geo/geosite/private.srs"
            },
            {
                 "tag": "xiaomiDNSblocklist",
                 "type": "remote",
                 "format": "source",
                 "url": "https://gist.githubusercontent.com/wyw1018/87499935e8de21b901cc283133ef69dc/raw/xiaomiDNSblocklist.json"
            },
            {
                 "tag": "geo_geosite_category-ads-all",
                 "type": "remote",
                 "format": "binary",
                 "url": "https://testingcf.jsdelivr.net/gh/MetaCubeX/meta-rules-dat@sing/geo/geosite/category-ads-all.srs"
            }
    ],
    "rules": [
      {
        "package_name": [
            "com.miui.notes",
            "com.sohu.inputmethod.sogou.xiaomi",
            "com.miui.micloudsyn",
            "com.miui.vpnsdkmanager",
            "com.miui.cloudservice",
            "com.miui.cloudbackup", 
            "com.miui.newmidrive",
            "com.miui.guardprovider",
            "com.miui.analytics",
            "com.xiaomi.market"
        ],
        "outbound": "block"
      },
      {
        "process_name": [
            "SogouTaskManager",
            "SogouServices",
            "SogouInput"
        ],
        "outbound": "block"
      },
      {
        "type": "logical",
        "mode": "or",
        "rules": [
          {
            "protocol": "dns"
          },
          {
            "port": 53
          }
        ],
        "outbound": "dns"
      },
            {
        "type": "logical",
        "mode": "or",
        "rules": [
          {
            "port": 853
          },
          {
            "network": "udp",
            "port": 443
          },
          {
            "protocol": [
              "stun"
            ]
          }
        ],
        "outbound": "block"
      },
      {
        "package_name": [
            "com.yahoo.mobile.client.android.mail"
        ],
        "outbound": "select"
      },
      {
         "outbound": "select",
         "domain_suffix": [
            "youtube.com",
            "googlevideo.com",
            "jsdelivr.com",
            "github.com",
            "sagernet.org",
            "githubusercontent.com"
          ]
      },
      {
         "outbound": "direct",
         "domain_suffix": [
            "cnbearing.net",
            "gaoloumi.cc"
         ]
      },
      {
        "rule_set": [
           "xiaomiDNSblocklist",
           "geo_geosite_category-ads-all"
        ],
        "outbound": "block"
      },
      {
         "rule_set": [
           "GeoSite-Google-CN"
         ],
         "outbound": "select"
      },
      {
         "clash_mode": "Direct",
         "outbound": "direct"
      },
      {
         "clash_mode": "Global",
         "outbound": "select"
      },
      {
        "domain": [
          "clash.razord.top",
          "yacd.metacubex.one",
          "yacd.haishan.me",
          "d.metacubex.one"
        ],
        "outbound": "direct"
      },
      {
        "ip_is_private": true,
        "outbound": "direct"
      },
      {
        "outbound": "direct",
        "rule_set": [
          "geosite-private",
          "geosite-geolocation-cn",
          "geoip-cn"
        ]
      }
    ]
  }
}
