{
  "dns": {
    "rules": [
      {  
         "outbound": "any",
         "server": "ALIDNS_DOH",
         "disable_cache": true
      },
      {
         "rule_set": [
           "GeoSite-Google-CN"
         ],
         "server": "GOOGLEDNS_DOH"
      },
      {
        "rule_set": [
          "GeoSite-CN",
          "geosite-geolocation-cn"
        ],
        "server": "ALIDNS_DOH"
      },
      {
        "rule_set": [
          "geoip-cn",
          "GeoIP-APNIC-CN-IPv4"
        ],
        "server": "GOOGLEDNS_DOH",
        "client_subnet": "61.139.2.1"
      }        
    ],
    "servers": [
      {
        "address": "https://8.8.8.8/dns-query",
        "strategy": "ipv4_only",
        "detour": "select",
        "tag": "GOOGLEDNS_DOH"
      },
      {
        "address": "https://223.5.5.5/dns-query",
        "strategy": "ipv4_only",
        "detour": "direct",
        "tag": "ALIDNS_DOH"
      }

    ],

    "independent_cache": true,
    "strategy": "ipv4_only"
  },
  "experimental": {
    "cache_file": {
      "enabled": true,
      "store_fakeip": false
    },
    "clash_api": {
      "external_controller": "127.0.0.1:9090",
      "external_ui": "xd",
      "external_ui_download_detour": "direct",
      "default_mode": "Rule",
      "external_ui_download_url": "https://mirror.ghproxy.com/https://github.com/MetaCubeX/metacubexd/archive/gh-pages.zip"
    }
  },
  "inbounds": [
    {
      "auto_route": true,
      "endpoint_independent_nat": false,
      "inet4_address": "172.19.0.1/30",
      "mtu": 9000,
      "sniff": true,
      "stack": "gvisor",
      "strict_route": true,
      "tag": "tun-in",
      "type": "tun"
    },
    {
      "type": "mixed",
      "listen": "127.0.0.1",
      "listen_port": 2080,
      "sniff": true,
      "users": []
    }
  ],
  "log": {
    "level": "debug",
    "timestamp": true
  },
  "outbounds": [
    {
      "tag": "select",
      "type": "selector",
      "outbounds": [
        "direct",
        "{all}"
      ]
    },
    {
      "tag": "direct",
      "type": "direct"
    },
    {
      "tag": "block",
      "type": "block"
    },
    {
      "tag": "dns",
      "type": "dns"
    }
  ],
  "route": {
    "auto_detect_interface": true,
    "rule_set": [
            {
                "tag": "GeoSite-CN",
                "type": "remote",
                "format": "binary",
                "url": "https://raw.githubusercontent.com/Dreista/sing-box-rule-set-cn/rule-set/accelerated-domains.china.conf.srs"
            },
            {
                "type": "remote",
                "tag": "geosite-geolocation-cn",
                "format": "binary",
                "url": "https://raw.githubusercontent.com/SagerNet/sing-geosite/rule-set/geosite-geolocation-cn.srs"
            },
            {
                "tag": "GeoSite-Google-CN",
                "type": "remote",
                "format": "binary",
                "url": "https://raw.githubusercontent.com/Dreista/sing-box-rule-set-cn/rule-set/google.china.conf.srs"
            },
            {
                "type": "remote",
                "tag": "geoip-cn",
                "format": "binary",
                "url": "https://raw.githubusercontent.com/SagerNet/sing-geoip/rule-set/geoip-cn.srs"
            },
            {
                "tag": "geosite-private",
                "type": "remote",
                "format": "binary",
                "url": "https://testingcf.jsdelivr.net/gh/MetaCubeX/meta-rules-dat@sing/geo/geosite/private.srs"
            }, 
            {
                "tag": "GeoIP-APNIC-CN-IPv4",
                "type": "remote",
                "format": "binary",
                "url": "https://raw.githubusercontent.com/Dreista/sing-box-rule-set-cn/rule-set/apnic-cn-ipv4.srs"
            },
            {
                 "tag": "geo_geosite_category-ads-all",
                 "type": "remote",
                 "format": "binary",
                 "url": "https://testingcf.jsdelivr.net/gh/MetaCubeX/meta-rules-dat@sing/geo/geosite/category-ads-all.srs"
            },    
            {
                "tag": "AdGuard-DNS-Filter",
                "type": "remote",
                "format": "binary",
                "url": "https://raw.githubusercontent.com/Dreista/sing-box-rule-set-cn/rule-set/filter.txt.srs"
            },
            {
                "tag": "AdGuard-DNS-Filter-Unblock",
                "type": "remote",
                "format": "binary",
                "url": "https://raw.githubusercontent.com/Dreista/sing-box-rule-set-cn/rule-set/filter.txt.unblock.srs"
            },
            {
                "tag": "hagezi-proplus",
                "type": "remote",
                "format": "binary",
                "url": "https://raw.githubusercontent.com/Phoroc/sing-rules/rule-set/hagezi-proplus.srs"
            },
            {
               "tag": "1hosts-pro",
               "type": "remote",
               "format": "binary",
               "url": "https://raw.githubusercontent.com/Phoroc/sing-rules/rule-set/1hosts-pro.srs"
            }
    ],
    "rules": [
      {
        "type": "logical",
        "mode": "or",
        "rules": [
          {
            "protocol": "dns"
          },
          {
            "port": 53
          }
        ],
        "outbound": "dns"
      },
      {
        "process_name": [
            "SearchApp.exe",
            "SogouTaskManager",
            "SogouServices",
            "SogouInput"
        ],
        "outbound": "block"
      },
            {
        "type": "logical",
        "mode": "or",
        "rules": [
          {
            "port": 853
          },
          {
            "network": "udp",
            "port": 443
          },
          {
            "protocol": [
              "stun",
              "turn"
            ]
          }
        ],
        "outbound": "block"
      },
      {
        "clash_mode": "Direct",
        "outbound": "direct"
      },
      {
        "clash_mode": "Global",
        "outbound": "select"
      },      
      {
         "outbound": "select",
         "domain_suffix": [
            "youtube.com",
            "googlevideo.com",
            "jsdelivr.com",
            "opendns.com",
            "googleusercontent.com",
            "githubusercontent.com"
          ]
      },
      {
         "outbound": "direct",
         "domain_suffix": [
            "cnbearing.net",
            "gaoloumi.cc"
         ]
      },
      {
        "rule_set": [
           "geo_geosite_category-ads-all",
           "AdGuard-DNS-Filter",
           "AdGuard-DNS-Filter-Unblock",
           "hagezi-proplus",
           "1hosts-pro"
        ],
        "outbound": "block"
      },
      {
         "rule_set": [
           "GeoSite-Google-CN"
         ],
         "outbound": "select"
      },
      {
        "domain": [
          "clash.razord.top",
          "yacd.metacubex.one",
          "yacd.haishan.me",
          "d.metacubex.one"
        ],
        "outbound": "direct"
      },
      {
        "ip_is_private": true,
        "outbound": "direct"
      },
      {
        "outbound": "direct",
        "rule_set": [
          "geosite-private",
          "GeoSite-CN",
          "geosite-geolocation-cn",
          "geoip-cn",
          "GeoIP-APNIC-CN-IPv4"
        ]
      }
    ]
  }
}
